{
  "name": "Restlet.js",
  "description": "RESTful web API framework for JavaScript",
  "version": "0.4.0",
  "author": "Thierry Templier <templth@yahoo.fr>",
  "contributors": [
  ],
  "license": "Apache",
  "repository": "restlet/restlet-framework-js",
  "keywords": [
    "restlet",
    "framework",
    "web",
    "rest",
    "restful",
    "router",
    "app",
    "api"
  ],
  "dependencies": {
  	"lodash": "3.8.0",
  	"moment": "~2.10",
  	"xml2js": "~0.4.0"
  },
  "devDependencies": {
    "mocha": "~2.2.1",
    "should": "~5.2.0",
    "jshint": "latest",
    "istanbul": "0.3.9",
    "jscs": "latest"
  },
  "files": [
    "LICENSE",
    "History.md",
    "Readme.md",
    "index.js",
    "lib/"
  ],
  "scripts": {
    "code:jshint": "jshint **/**.js",
    "code:style": "node_modules/jscs/bin/jscs *.js lib/*.js test/server/*.js",
    "test:default": "node_modules/mocha/bin/_mocha --report lcovonly --recursive --reporter spec test/server --check-leaks test/server",
    "test:ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --require test/ --reporter spec --check-leaks test/",
    "test:cov": ""
  }
}